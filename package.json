{
  "name": "test-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn db && serverless offline start --port 4200 --useSeparateProcesses",
    "deploy": "serverless deploy -v",
    "deploy:prod": "serverless deploy -v -s prod",
    "test": "mocha -r ts-node/register 'src/**/*spec.*' --exit",
    "test:u": "mocha -r ts-node/register 'src/**/*.spec.*' --exit",
    "test:i": "mocha -r ts-node/register 'src/**/*.ispec.*' --exit",
    "db": "scripts/start_db.sh",
    "db:kill": "pgrep mongod | xargs kill",
    "ingest": "serverless invoke local -f ingest-games"
  },
  "dependencies": {
    "@types/lodash": "^4.14.126",
    "axios": "^0.18.0",
    "elasticsearch": "^15.4.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.6",
    "serverless": "^1.42.1",
    "url-parse": "^1.4.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.23",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.7",
    "@types/elasticsearch": "^5.0.32",
    "@types/joi": "^14.3.2",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.5.0",
    "@types/node": "^11.11.3",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.10",
    "@types/sinon-chai": "^3.2.2",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "chai": "^4.2.0",
    "chai-sorted": "^0.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^6.0.2",
    "prettier": "^1.17.0",
    "proxyquire": "^2.1.0",
    "serverless-offline": "^4.7.1",
    "serverless-plugin-typescript": "^1.1.5",
    "sinon": "^7.3.0",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.5"
  }
